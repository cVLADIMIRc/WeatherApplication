                <ComboBoxItem>SADADAD</ComboBoxItem>
            </ComboBox>
            <Border Padding="3" HorizontalAlignment="Center"  VerticalAlignment="Top" BorderBrush="Azure" BorderThickness="3" Grid.Row="1">
